<api:ObservableUserControl
	x:Class="PEunion.TabRtlo"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:local="clr-namespace:PEunion"
	xmlns:api="clr-namespace:BytecodeApi;assembly=BytecodeApi"
	mc:Ignorable="d">
	<Grid Margin="10">
		<StackPanel>
			<GroupBox Header="Source File">
				<local:FileBrowser HorizontalAlignment="Left" FilesSelect="ctrlBrowseSourceFile_FilesSelect" />
			</GroupBox>
		</StackPanel>
	</Grid>
</api:ObservableUserControl>